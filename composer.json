{
    "name"       : "samadmin/samadmin",
    "description": "a new framework use for myself",
    "type"       : "project",
    "keywords"   : [
        "framework",
        "thinkphp",
        "ORM",
        "SamAdmin"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name" : "sam",
            "email": "sam_zz@126.com"
        }
    ],
    "require": {
        "php"                       : ">=5.6.0",
        "topthink/framework"        : "5.1.*",
        "topthink/think-captcha"    : "^2.0",
        "zoujingli/wechat-developer": "^1.2",
        "topthink/think-image"      : "^1.0",
        "anerg2046/sns_auth"        : "^2.0",
        "jaeger/querylist"          : "^4.1"
    },
    "autoload": {
        "psr-4": {
            "app\\": "application"
        }
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url" : "https://packagist.phpcomposer.com"
        }
    }
}
